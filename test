encode_test "MSB 8 Block" ./ans-coder "-e -m -b -a 16 -M 8 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "MSB2 8 Block" ./ans-coder "-e -n -b -a 16 -M 8 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "Sorted MSB 8 Block" ./ans-coder "-e -m -b -a 16 -M 8 -S fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "Partially Sorted MSB 6 Block" ./ans-coder "-e -m -b -a 16 -M 8 -k 50 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
