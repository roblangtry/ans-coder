encode_test "MSB 6 Block" ./ans-coder "-e -m -b -a 16 -M 6 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "MSB2 6 Block" ./ans-coder "-e -n -b -a 16 -M 6 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "Sorted MSB 6 Block" ./ans-coder "-e -m -b -a 16 -M 6 -S fcc.data _g9-enc" "-d _g9-enc _g9-dec"
encode_test "Partially Sorted MSB 6 Block" ./ans-coder "-e -m -b -a 16 -M 6 -k 50 fcc.data _g9-enc" "-d _g9-enc _g9-dec"
