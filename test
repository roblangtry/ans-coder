#!/bin/bash
make build_profile
function encode_test {
    echo "-----------------------------"
    echo " $1"
    echo "-----------------------------"
    echo $2 $3
    echo $2 $4
    TIME=$( { time $2 $3; } 2>&1 )
    TVAL=$(echo $TIME | sed 's/real 0m\([0-9.]*\)s.*/\1/')
    echo "Encoding  - $TVAL"
    TIME=$( { time $2 $4; } 2>&1 )
    TVAL=$(echo $TIME | sed 's/real 0m\([0-9.]*\)s.*/\1/')
    echo "Decoding  - $TVAL"
    val="Failure!"
    cmp _g9-dec fcc.data && val="Success!"
    echo "Comparing - $val"
    LL=$(ls -l _g9-enc)
    val=$(echo $LL | sed 's/.*root //g' | sed 's/ .*//')
    echo "Size      - $val"
    rm _g9-dec
    rm _g9-enc
}
function encode2_test {
    echo "-----------------------------"
    echo " $1"
    echo "-----------------------------"
    echo $2 $3
    echo $2 $4
    TIME=$( { time $2 $3 < fcc.data > _g9-enc; } 2>&1 )
    TVAL=$(echo $TIME | sed 's/real 0m\([0-9.]*\)s.*/\1/')
    echo "Encoding  - $TVAL"
    TIME=$( { time $2 $4 < _g9-enc > _g9-dec; } 2>&1 )
    TVAL=$(echo $TIME | sed 's/real 0m\([0-9.]*\)s.*/\1/')
    echo "Decoding  - $TVAL"
    val="Failure!"
    cmp _g9-dec fcc.data && val="Success!"
    echo "Comparing - $val"
    LL=$(ls -l _g9-enc)
    val=$(echo $LL | sed 's/.*root //g' | sed 's/ .*//')
    echo "Size      - $val"
    rm _g9-dec
    rm _g9-enc
}

